<VirtualHost *:80>
	ServerAdmin webmaster@localhost
	ServerName ip13.tk
	ServerAlias www.ip13.tk


	DocumentRoot /home/Projet_IP/Symfony_PHP_project/web
	<Directory /home/Projet_IP/Symfony_PHP_project/web>
		AllowOverride All
		Order Allow,Deny
		Allow from All


			
			
			
		

	</Directory>







	ScriptAlias /cgi-bin/ /usr/lib/cgi-bin/
	<Directory "/usr/lib/cgi-bin">
		AllowOverride None
		Options +ExecCGI -MultiViews +SymLinksIfOwnerMatch
		Order allow,deny
		Allow from all

	




	ErrorLog ${APACHE_LOG_DIR}/Symfony_PHP_project_error.log

	# Possible values include: debug, info, notice, warn, error, crit,
	# alert, emerg.
	LogLevel warn

	CustomLog ${APACHE_LOG_DIR}/Symfony_PHP_project_access.log combined
</VirtualHost>
